{
  "name": "@bite-track/api",
  "version": "0.0.0",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "private": true,
  "scripts": {
    "build": "nest build && yarn gen:types",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "start": "nest start",
    "start:test": "NODE_ENV=test nest start",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test:unit": "jest --config ./src/__tests__/jest.unit.config.ts --forceExit --passWithNoTests --json --outputFile=./src/__tests__/reports/unit-report.json",
    "test:unit:debug": "node --inspect-brk --max_old_space_size=4096 node_modules/.bin/jest --config ./src/__tests__/jest.unit.config.ts --forceExit --passWithNoTests --json --outputFile=./src/__tests__/reports/unit-report.json",
    "test:integration": "jest --config ./src/__tests__/jest.integrations.config.ts --forceExit --passWithNoTests --json --outputFile=./src/__tests__/reports/integration-report.json",
    "test:integration:debug": "node --inspect-brk --max_old_space_size=4096 node_modules/.bin/jest --config ./src/__tests__/jest.integrations.config.ts --runInBand --forceExit --passWithNoTests --json --outputFile=./src/__tests__/reports/integration-report.json",
    "gen:types": "nest start --config ./nest-swagger-cli.json"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "nest-winston": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@bite-track/eslint-config": "*",
    "@bite-track/types": "*",
    "@bite-track/typescript-config": "*",
    "@hey-api/openapi-ts": "^0.48.2",
    "@nestjs/testing": "^10.3.9",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nest-winston": "^1.10.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
